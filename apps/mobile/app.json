{
  "expo": {
    "name": "Flixor",
    "slug": "flixor-mobile",
    "scheme": "flixor",
    "version": "0.1.8",
    "orientation": "default",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "xyz.flixor.mobile",
      "icon": "./assets/icons/ios-light.png",
      "jsEngine": "hermes",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "xyz.flixor.mobile",
      "usesCleartextTraffic": true,
      "jsEngine": "hermes",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "monochromeImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "extra": {
      "eas": {
        "projectId": "14e1896c-9a1f-4f6b-a9c1-f5b8c5a9d2b4"
      }
    },
    "owner": "deviprasad97",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true,
            "minSdkVersion": 26,
            "ndkVersion": "27.1.12297006"
          }
        }
      ],
      "./plugins/withKSPlayer",
      "./plugins/withMPVPlayer"
    ]
  }
}
